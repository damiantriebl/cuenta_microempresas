      "shortName": "cuentamicroempresas",
      "lang": "es",
      "scope": "/",
      "themeColor": "#25B4BD",
      "backgroundColor": "#ffffff",
      "display": "standalone",
      "orientation": "portrait",
      "startUrl": "/"
    },
    "extra": {
      "googleClientId": "752493866551-92vnat1ff88sgprnadjukd816p1cjsqp.apps.googleusercontent.com",
      "router": {},
      "eas": {
        "projectId": "3dc127bf-00d4-4447-a8ea-eaaeae7a6276"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "expo": {
       "android": {
         "package": "com.ukn.cuentamicroempresas", "versionCode": 3 

{
  "name": "micro-empresas",
  "main": "expo-router/entry",
  "version": "1.0.1",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "build:web": "expo export --platform web",
    "deploy": "npm run build:web && firebase deploy",
    "preview:web": "expo export --platform web && npx serve dist",
    "test": "jest --watchAll",
    "test:simple": "jest --config jest.config.simple.js",
    "test:fallback": "jest --config jest.config.simple.js --testNamePattern='Fallback Icons'",
    "lint": "expo lint",
    "migrate:execute": "tsx scripts/execute-migration.ts",
    "migrate:dry-run": "tsx scripts/execute-migration.ts --dry-run",
    "migrate:rollback": "tsx scripts/rollback-migration.ts",
    "migrate:test": "tsx scripts/test-migration.ts",
    "migrate:production": "tsx scripts/production-migration.ts",
    "prepare:production": "node scripts/prepare-production.js",
    "build:android:preview": "eas build --platform android --profile preview",
    "build:android:production": "eas build --platform android --profile production",
    "submit:android": "eas submit --platform android --profile production",
    "deploy:android": "npm run build:android:production && npm run submit:android",
    "android:prebuild": "npx expo prebuild --platform android",
    "android:dev": "npx expo run:android",
    "android:release": "npx expo run:android --variant release",
    "android:build-apk": "cd android && gradlew.bat assembleRelease",
    "android:build-aab": "cd android && gradlew.bat bundleRelease",
    "android:clean": "cd android && gradlew.bat clean",
    "build:web": "expo export --platform web",
    "deploy:web": "npm run build:web && firebase deploy --only hosting",
    "preview:web": "firebase serve --only hosting"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    }
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.28.3",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/datetimepicker": "8.4.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "4.5.6",
    "@react-navigation/bottom-tabs": "^7.0.0",
    "@react-navigation/native": "^7.0.0",
    "expo": "53.0.22",
    "expo-auth-session": "~6.2.1",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-crypto": "^14.1.5",
    "expo-device": "^7.1.4",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-linking": "~7.1.7",
    "expo-notifications": "^0.31.4",
    "expo-router": "~5.1.6",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "firebase": "^11.0.1",
    "react": "19.0.0",
    "react-color": "^2.19.3",
    "react-datepicker": "^7.5.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-color-picker": "^0.6.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "^0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/react": "^19.0.14",
    "@types/react-native": "^0.73.0",
    "@types/react-test-renderer": "^19.0.0",
    "commander": "^12.0.0",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~9.2.0",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.10",
    "react-test-renderer": "19.0.0",
    "tsx": "^4.7.0",
    "typescript": "~5.8.3"
  },
  "private": true,
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
